{
  "name": "@tern-secure/nextjs",
  "version": "3.2.43",
  "packageManager": "npm@10.9.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TernSecure/nextjs.git"
  },
  "bugs": {
    "url": "https://github.com/TernSecure/nextjs/issues"
  },
  "homepage": "https://github.com/tern-secure/nextjs#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup && tsc -p tsconfig.add.json",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "firebase": "^10.0.0",
    "firebase-admin": "^12.5.0",
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "react": "^18 || ^19.0.0-0",
    "react-dom": "^18 || ^19.0.0-0"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./server": {
      "types": "./dist/types/app-router/server/index.d.ts",
      "import": "./dist/esm/app-router/server/index.js",
      "require": "./dist/cjs/app-router/server/index.js"
    }
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  }
