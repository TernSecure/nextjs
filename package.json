  "peerDependenciesMeta": {
    "tailwindCSS": {
      "optional": true
    },
    "postcss": {
      "optional": true
    },
    "autoprefixer": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
